User
    email Text Asc
    password Text
    name Text
    level Level
    firstAdd UTCTime
    UniqueEmail email
    deriving Show

Room
    number Text
    level Level
    available Bool
    validTime Day
    firstAdd UTCTime Asc
    UniqueRoomNo number
    deriving Show

Record
    userId UserId
    day Day Desc
    roomId RoomId
    startTime TimeOfDay
    endTime TimeOfDay
    bookingTime UTCTime
    roomUsage RoomUsage
    otherUsage Text Maybe
    cancel Bool default=false
    UniqueRecord day roomId startTime endTime
    deriving Show

DayRecords
    ids [RecordId]
    day Day Desc
    UniqueDay day 
    deriving Show

Bulletin
    createTime UTCTime
    

-- By default this file is used in Model.hs (which is imported by Foundation.hs)
